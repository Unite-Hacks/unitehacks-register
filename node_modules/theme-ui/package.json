  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0"
  },
  "keywords": [
    "theme-ui",
    "emotion",
    "mdx",
    "css",
    "styles",
    "css-in-js"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "jsx-dev-runtime.ts",

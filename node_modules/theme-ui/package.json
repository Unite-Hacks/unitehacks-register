{
  "name": "theme-ui",
  "version": "0.10.0",
  "description": "The Design Graph Framework",
  "source": "src/index.ts",
  "main": "dist/theme-ui.cjs.js",
  "module": "dist/theme-ui.esm.js",
  "types": "dist/theme-ui.cjs.d.ts",
